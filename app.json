{
  "expo": {
    "name": "Kaz ni Kaz",
    "slug": "KazniKaz",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#da9f21"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ngoga.KazniKaz",
